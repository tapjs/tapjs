{
  "name": "@tapjs/core",
  "version": "4.0.6",
  "description": "pluggable core of node-tap",
  "tshy": {
    "main": true,
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./proc": "./src/proc.ts"
    }
  },
  "type": "module",
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./proc": {
      "import": {
        "types": "./dist/esm/proc.d.ts",
        "default": "./dist/esm/proc.js"
      },
      "require": {
        "types": "./dist/commonjs/proc.d.ts",
        "default": "./dist/commonjs/proc.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "snap": "tap",
    "test": "tap",
    "prepare": "bash -c 'if [ -d src ]; then tshy; fi'",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "format": "prettier --write . --log-level warn --ignore-path ../../.prettierignore --cache",
    "typedoc": "typedoc"
  },
  "author": "Isaac Z. Schlueter <i@izs.me> (https://blog.izs.me)",
  "license": "BlueOak-1.0.0",
  "dependencies": {
    "@tapjs/processinfo": "^3.1.8",
    "@tapjs/stack": "4.0.2",
    "@tapjs/test": "4.0.6",
    "async-hook-domain": "^4.0.1",
    "diff": "^8.0.2",
    "is-actual-promise": "^1.0.1",
    "minipass": "^7.0.4",
    "signal-exit": "4.1",
    "tap-parser": "18.0.2",
    "tap-yaml": "4.0.2",
    "tcompare": "9.0.2",
    "trivial-deferred": "^2.0.0"
  },
  "tap": {
    "coverage-map": "map.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapjs/tapjs.git"
  },
  "module": "./dist/esm/index.js",
  "engines": {
    "node": "20 || >=22"
  }
}
