{
  "name": "tap-parser",
  "version": "1.2.2",
  "description": "parse the test anything protocol",
  "main": "index.js",
  "bin": {
    "tap-parser": "bin/cmd.js"
  },
  "dependencies": {
    "events-to-array": "^1.0.1",
    "js-yaml": "^3.2.7"
  },
  "devDependencies": {
    "glob": "^5.0.2",
    "tap": "4 || 5",
    "tape": "^3.5.0"
  },
  "scripts": {
    "test": "tap test/*.js --cov",
    "regen-fixtures": "node scripts/generate-test.js test/fixtures/*.tap"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/10",
      "chrome/latest",
      "firefox/3.5",
      "firefox/latest",
      "opera/latest",
      "safari/latest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/substack/tap-parser.git"
  },
  "homepage": "https://github.com/substack/tap-parser",
  "keywords": [
    "tap",
    "test",
    "parser"
  ],
  "author": {
    "name": "James Halliday",
    "email": "mail@substack.net",
    "url": "http://substack.net"
  },
  "license": "MIT",
  "optionalDependencies": {
    "readable-stream": "^2"
  },
  "files": [
    "index.js",
    "bin/cmd.js",
    "bin/usage.txt"
  ]
}
